{
	"name": "kdyby/datetime-provider",
	"type": "library",
	"description": "Time provider, providing consistent current date, time, datetime and timezone across the request.",
	"keywords": ["kdyby", "date", "time", "datetime", "microseconds", "clock", "provider"],
	"homepage": "http://kdyby.org",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Michael Moravec"
		},
		{
			"name": "Richard Ejem"
		},
		{
			"name": "Filip Proch√°zka",
			"homepage": "http://filip-prochazka.com"
		},
		{
			"name": "Kdyby Community",
			"homepage": "https://github.com/Kdyby/DateTimeProvider/graphs/contributors"
		}
	],
	"support": {
		"issues": "https://github.com/Kdyby/DateTimeProvider/issues"
	},
	"require": {
		"php": "^7.1",
		"kdyby/strict-objects": "^1.0"
	},
	"require-dev": {
		"kdyby/coding-standard": "^1.0@dev",
		"nette/tester": "~1.7",
		"phpstan/phpstan-shim": "^0.7"
	},
	"minimum-stability": "dev",
	"autoload": {
		"psr-4": {
			"Kdyby\\DateTimeProvider\\": "src/"
		},
		"classmap": [
			"src/exceptions.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"KdybyTests\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	}
}
